{
    "cSpell.words": [
        "dnum",
        "dotenv",
        "edms",
        "fdos",
        "FRBR",
        "guids",
        "mref",
        "pywebviewready",
        "qstr",
        "Rdate",
        "rref",
        "Vite"
    ],
    "[typescriptreact]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.enable": true,

  "[python]": {
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
    "files.associations": {
        "*.spec": "python"
    },
}
