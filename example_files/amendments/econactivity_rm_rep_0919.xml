     <amendmentBody GUID="_f25d7b40-be37-41a4-afc9-bf60f720dbd4" eId="amnd_6" ukl:location="" ukl:targetPage="p_13__ln_5" ukl:targetProvision="sch" ukl:type="insert">
      <amendmentHeading GUID="_cac2f382-1a68-4d29-853f-cdd5bab61cef" eId="amnd_6__hdg">
       <block name="proposer"><docIntroducer refersTo="amnd_6#person-4495">Kit Malthouse</docIntroducer></block>
       <block name="supporters"><docProponent refersTo="amnd_6#person-1502">Mr David Jones</docProponent><docProponent refersTo="amnd_6#person-4805">Alicia Kearns</docProponent><docProponent refersTo="amnd_6#person-4487">Kelly Tolhurst</docProponent><docProponent refersTo="amnd_6#person-55">Sir Desmond Swayne</docProponent><docProponent refersTo="amnd_6#person-3990">Richard Graham</docProponent><docProponent refersTo="amnd_6#person-4429">Mr William Wragg</docProponent><docProponent refersTo="amnd_6#person-4459">Mrs Flick Drummond</docProponent><docProponent refersTo="amnd_6#person-104">Crispin Blunt [R]</docProponent></block>
      </amendmentHeading>
      <amendmentContent GUID="_8f493c54-b1c0-498a-a3e2-88f17a4d8923" eId="amnd_6__cont">
       <tblock>
        <num ukl:dnum="HoC12">6</num>
        <block name="instruction"> <mod>Schedule, page 13, line 5, at end insert—<quotedStructure GUID="_b4932d7a-88d3-4be5-92bc-98440d22d94b" eId="amnd_6__cont__qstr" endQuote="”" startQuote="“" ukl:context="schedule" ukl:docName="ukpga" ukl:indent="indent0">
         <paragraph GUID="_56a37adf-9e1b-4838-a7cd-06c478efde8d" class="schProv1" eId="amnd_6__cont__qstr__para_6A">
          <num ukl:autonumber="no">6A</num>
          <content>
           <p>Section 1 does not prevent regard to a consideration so far as it relates to conduct which it is the position of His Majesty’s Government represents a breach of international law.</p>
          </content>
         </paragraph>
         </quotedStructure><inline name="AppendText"/></mod> </block>
       </tblock>
      </amendmentContent>
      <amendmentJustification>
       <blockContainer class="explanatoryStatement">
        <heading GUID="_7be51484-933c-41c0-a77f-a1eca66963da" eId="amnd_6__hdg__oc_1">Member's explanatory statement</heading>
        <p>This amendment would permit decisions which would otherwise be in breach of clause 1 if they are taken in response to conduct which the Government considers to be a breach of international law.</p>
       </blockContainer>
      </amendmentJustification>
     </amendmentBody>
    </amendment>
